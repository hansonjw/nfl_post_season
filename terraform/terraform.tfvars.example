aws_region = "us-west-2"
project_name = "nfl_post_season"

# Add your admin email(s) here
admin_emails = [
  "your-email@gmail.com"
]

# Google OAuth credentials (get from Google Cloud Console)
google_client_id = "your-google-client-id.apps.googleusercontent.com"
google_client_secret = "your-google-client-secret"

# Allowed CORS origins (update with your frontend domain)
allowed_origins = [
  "http://localhost:5173",
  "https://your-domain.com"
]

# Enable write endpoints for local development (set to false for production)
enable_write_endpoints = false

# Admin passkey for write operations (required if enable_write_endpoints is true)
admin_passkey = "your-secret-passkey-here"
